
@{
    ViewBag.Title = "listarSolicitudes";
}
@section scripts {

    <script src="~/Scripts/page.js"></script>

}


<div class="table-responsive">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Selecionar</th>
                <th>Cliente</th>
                <th>Fecha Servicio</th>
                <th>Tipo Servicio</th>
                <th>Opciones</th>
            </tr>
        </thead>
        <tbody>
@foreach (var item in (List<EntityLayer.Solicitud>)ViewData["Solicitudes"])
{
    
            <tr>
                <th scope="row"><input type="checkbox" name="solicit" value="@item.nroSolicitud"/></th>
                <td>@item.razonCliente</td>
                <td>@item.fechaServicio</td>
                <td>@item.nomTipoServicio</td>
                <td><a href="#">Ver Detalle</a></td>
            </tr>
}
           
        </tbody>
    </table>
</div>